{
  "name": "simple-preact-ssr-boilerplate",
  "version": "1.0.0",
  "description": "simple-preact-ssr-boilerplate",
  "main": "index.js",
  "scripts": {
    "prod": "npm-run-all build:client-prod build:server-prod start:ssr-server-prod",
    "build:client-prod": "webpack --config=config/webpack.client.prod.js",
    "build:client-dev": "webpack --config=config/webpack.client.dev.js",
    "build:server-prod": "webpack --config=config/webpack.server.prod.js",
    "build:server-dev": "webpack --config=config/webpack.server.dev.js",
    "start:ssr-server-prod": "node ./dist/server.js",
    "start:ssr-server-dev": "webpack --config ./config/webpack.server.dev.js",
    "analyze": "cross-env ENABLE_ANALYZER=true npm run build:client"
  },
  "keywords": [
    "preact",
    "ssr",
    "boilerplate",
    "simple"
  ],
  "author": "azakaWow",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "preact": "^10.4.6",
    "preact-render-to-string": "^5.1.10",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "css-loader": "^4.0.0",
    "css-minimizer-webpack-plugin": "^1.1.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon-webpack-plugin": "^4.3.2",
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^3.0.8",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^2.5.0"
  }
}
